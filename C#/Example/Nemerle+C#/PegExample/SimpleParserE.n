using System;
using System.Collections.Generic;
using Nemerle.Collections;
using Nemerle.Peg;

[PegGrammar(option=none, number,grammar {
  d      : int = ['0'..'9'];
  number : int = d+;
})]
class SimpleParserE
{
  d(token : NToken) : int {
    int.Parse(GetText(token))
  }
  
  number(digits : List[int]) : int {
    digits.FoldRight(0,(i,acc)=>acc*10+i)
  }
}